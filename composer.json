{
  "name": "twingly/twingly-search",
  "description": "Twingly Search API for PHP",
  "license": "MIT",
  "keywords": [
    "twingly",
    "twingly.com",
    "api"
  ],
  "authors": [
    {
      "name": "Twingly AB",
      "email": "support@twingly.com"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "guzzlehttp/guzzle": ">=7.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "9.*",
    "php-vcr/php-vcr": "1.5.*",
    "phpdocumentor/phpdocumentor": ">=3.1"
  },
  "autoload": {
    "psr-4": {
      "Twingly\\": "src"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true
    }
  }
}
